{
  "name": "omnium",
  "version": "0.1.0",
  "private": true,
  "proxy": {
    "/auth/google": {
      "target": "http://localhost:3000"
    },
    "/auth/facebook": {
      "target": "http://localhost:3000"
    },
    "/api/*": {
      "target": "http://localhost:3000"
    },
    "socket.io/*": {
      "target": "http://localhost:3000"
    }
  },
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.18.2",
    "dotenv": "^5.0.1",
    "form-data": "^2.3.2",
    "react": "^16.2.0",
    "react-beautiful-dnd": "^7.0.0",
    "react-bootstrap": "^0.32.1",
    "react-dom": "^16.2.0",
    "react-facebook-login": "^4.0.1",
    "react-google-login": "^3.0.4",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.1",
    "react-share": "^2.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
